	file = {arXiv\:1412.0722 PDF:C\:\\Users\\Melanie\\Zotero\\storage\\JJ2HHE72\\Hamilton and Casey - 2016 - Creation of a high spatiotemporal resolution globa.pdf:application/pdf;arXiv.org Snapshot:C\:\\Users\\Melanie\\Zotero\\storage\\8M47QWPI\\1412.html:text/html}
