@article{tournadre_anthropogenic_2014,
	title = {Anthropogenic pressure on the open ocean: The growth of ship traffic revealed by altimeter data analysis},
	volume = {41},
	issn = {00948276},
	url = {http://doi.wiley.com/10.1002/2014GL061786},
	doi = {10.1002/2014GL061786},
	shorttitle = {Anthropogenic pressure on the open ocean},
	abstract = {Marine ecosystems are under increasing anthropogenic pressures from marine and terrestrial activities. Ship traﬃc, the major cause of change in the open ocean, and its temporal evolution are still largely unknown because of lack of data. Altimeter data provide a new powerful tool to detect and monitor the ship traﬃc through a method of analysis of echo waveform. The archive of seven altimeter missions has been processed to create a two decade database of ship locations. The estimated annual density maps compare well with the ones obtained from Automatic Identiﬁcation System. The ship traﬃc analysis shows a global fourfold growth between 1992 and 2012, the largest increase being observed in the Indian Ocean and the Chinese seas reﬂecting the world trade change. Although mainly concentrated along lanes, the traﬃc has a direct impact on the atmosphere, e.g., on the growth of tropospheric nitrogen dioxide in the Indian Ocean.},
	pages = {7924--7932},
	number = {22},
	journaltitle = {Geophysical Research Letters},
	author = {Tournadre, J.},
	urldate = {2018-10-24},
	date = {2014-11-28},
	langid = {english},
	file = {Tournadre - 2014 - Anthropogenic pressure on the open ocean The grow.pdf:C\:\\Users\\Melanie\\Zotero\\storage\\MFBS689N\\Tournadre - 2014 - Anthropogenic pressure on the open ocean The grow.pdf:application/pdf}
