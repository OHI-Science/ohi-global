	date = {2012},
	file = {Guerry et al. - 2012 - Modeling benefits from nature using ecosystem ser.pdf:C\:\\Users\\Melanie\\Zotero\\storage\\J9DAPNVM\\Guerry et al. - 2012 - Modeling benefits from nature using ecosystem ser.pdf:application/pdf;T&F Snapshot:C\:\\Users\\Melanie\\Zotero\\storage\\9HQCG737\\Guerry et al. - 2012 - Modeling benefits from nature using ecosystem ser.html:text/html}
}

