@article{halpern2022cumulative,
	title = {The cumulative environmental footprint of global food production},
	abstract = {Feeding humanity puts enormous environmental pressure on our planet. These pressures are unequally distributed, yet we have piecemeal knowledge of how they accumulate across marine, freshwater and terrestrial systems. Here we present global geospatial analyses detailing individual and cumulative footprints of 99\% of total reported production of aquatic and terrestrial foods. On land, we find five countries contribute nearly half of food’s cumulative environmental pressure. Aquatic systems produce only 1.1\% of food but 9.7\% of global cumulative pressure. Which pressures drive cumulative environmental footprint varies substantially by food and country. Importantly, the environmental
efficiency of producing a particular food type also varies spatially, such that rankings of foods by efficiency differ sharply among countries. These disparities provide the foundation for efforts to steer consumption toward lower-impact foods, and ultimately the system-wide restructuring essential for sustainably feeding humanity.},
