	number = {11},
	journaltitle = {{PLOS} {ONE}},
	shortjournal = {{PLoS} {ONE}},
	author = {Tuholske, Cascade and Halpern, Benjamin S. and Blasco, Gordon and Villasenor, Juan Carlos and Frazier, Melanie and Caylor, Kelly},
	editor = {Veettil, Bijeesh Kozhikkodan},
